{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "A setup to bootstrap my react apps fast",
  "author": "Szymon Wiszczuk",
  "license": "MIT",
  "dependencies": {
    "re": "^0.1.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-resizable": "^1.10.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html -d build",
    "mock-server": "yarn run json-server --watch db.json",
    "all": "yarn run start && yarn run mock-server",
    "purge": "rm -rf node_modules && rm -rf .cache && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.9",
    "@types/react-resizable": "^1.7.2",
    "json-server": "^0.16.1",
    "parcel-bundler": "^1.12.2",
    "sass": "^1.26.2",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "resolutions": {
    "@babel/helper-compilation-targets": "7.8.4"
  }
}
